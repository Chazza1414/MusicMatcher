entity Song {
    id Long required
    artist String required
    title String required
}

entity LikedSong {
    id Long required
    chosen Boolean required
}

entity DislikedSong {
    id Long required
    chosen Boolean required
}

entity MainPage {
    id Long required
}

relationship ManyToOne {
    Song{mainPage(id)} to MainPage
}

relationship OneToOne {
    MainPage to User
    Song{likedSong(chosen)} to LikedSong{song(id)}
    Song{dislikedSong(chosen)} to DislikedSong{song(id)}
}
